<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
   http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">
   
   	<bean id="VendorDelegate" class="kikakuya.delegate.VendorDelegate">
		<property name="vendorService" ref="vendorService"></property>
	</bean>
	
	<bean id="vendorService" class="kikakuya.service.implementation.VendorServiceImpl">
		<property name="vendorDao" ref="vendorDao"></property>
	</bean>
		
	<bean name="vendorDao" class="kikakuya.dao.implementation.VendorDaoImpl">
		<property name="dataSource" ref="dataSource"></property>
	</bean>
</beans>